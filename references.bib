@article{Gargiulo2008,
abstract = {Robotic operations are one of the major maintenance challenges for ITER and future fusion reactors. In particular, in-vessel inspection operations without loss of conditioning will be mandatory. In this context, an Articulated Inspection Arm (AIA) is currently developed by the CEA within the European work programme framework, which aims at demonstrating the feasibility of a multi-purpose in-vessel Remote Handling inspection system using a long reach, limited payload carrier (up to 10 kg). It is composed of 5 segments with 8 degrees of freedom and a total range of 8 m. The first in situ tests will take place by the end of 2007 on the Tore Supra Tokamak at Cadarache (France). They will validate concepts for operations under ITER relevant vacuum and temperature conditions. After qualification, the arm will constitute a promising tool for various applications. Several processes are already considered for ITER maintenance and will be demonstrated on the AIA robot carrier: • The first embedded process is the viewing system. It is already manufactured and will allow close visual inspection of the complex Plasma Facing Components (PFC) (limiters, neutralisers, RF antenna, diagnostic windows, etc.). • In situ localisation of water leakage based on a helium sniffing system is also being studied to improve and facilitate maintenance operations. • Finally a laser ablation system for PFC detritiation, developed in CEA laboratories, is being fitted to be implemented on the robot for future operation in Tore Supra. This paper deals with the integration of the robot into Tore Supra and the progress in the development of the processes listed above. It also describes the current test campaign aiming to qualify the robot performance and reliability under vacuum and temperature conditions.},
author = {Gargiulo, Laurent and Bayetti, Pascal and Bruno, Vincent and Cordier, Jean-Jacques and Friconneau, Jean-Pierre and Grisolia, Christian and Hatchressian, Jean-Claude and Houry, Michael and Keller, Delphine and Perrot, Yann},
doi = {10.1016/j.fusengdes.2008.06.039},
issn = {09203796},
journal = {Fusion Engineering and Design},
keywords = {articulated inspection arm,laser ablation,maintenance,remote handling,robot,viewing process},
month = dec,
number = {10-12},
pages = {1833--1836},
title = {{Development of an ITER relevant inspection robot}},
url = {http://dx.doi.org/10.1016/j.fusengdes.2008.06.039},
volume = {83},
year = {2008}
}
@article{Gargiulo2009,
abstract = {Robotic operations are one of the major maintenance challenges for ITER and future fusion reactors. CEA has developed a multipurpose carrier able to realize deployments in the plasma vessel without breaking the Ultra High Vacuum (UHV) and temperature conditioning. A 6 years R\&D programme was jointly conducted by CEA-LIST Interactive Robotics Unit and the Institute for Magnetic Fusion Research (IRFM) in order to demonstrate the feasibility and reliability of an in-vessel inspection robot relevant to ITER requirements. The Articulated Inspection Arm robot (AIA) is an 8-m long multilink carrier with a payload up to 10 kg operable between plasma under tokamak conditioning environment; its geometry allows a complete close inspection of Plasma Facing Components (PFCs) of the Tore Supra vessel. Different tools are being developed by CEA to be plugged at the front head of the carrier. The diagnostic presently in operation consists in a viewing system offering accurate visual inspection of PFCs. Leak detection of first wall based on helium sniffing and laser compact system for carbon co-deposited layers characterizations or treatments are also considered for demonstration. In April 2008, the AIA robot equipped with its vision diagnostic has realized a complete deployment into Tore Supra and the first closed inspection of the vessel under UHV conditions. During the upcoming experimental campaign, the same operation will be performed under relevant conditions (10-6 Pa and 120 °C) after a conditioning phase at 200 °C to avoid outgassing pollution of the chamber. This paper describes the different steps of the project development, robot capabilities with the present operations conducted on Tore Supra and future requirements for making the robot a tool for tokamak routine operation.},
author = {Gargiulo, Laurent and Bayetti, Pascal and Bruno, Vincent and Hatchressian, Jean-Claude and Hernandez, Caroline and Houry, Michael and Keller, Delphine and Martins, Jean-Pierre and Measson, Yvan and Perrot, Yann},
doi = {10.1016/j.fusengdes.2008.11.043},
issn = {09203796},
journal = {Fusion Engineering and Design},
keywords = {aia,articulated inspection arm,diagnostic,fusion,iter,laser ablation,maintenance,remote handling,robot,tokamak,tore supra,vision},
month = jun,
number = {2-6},
pages = {220--223},
title = {{Operation of an ITER relevant inspection robot on Tore Supra tokamak}},
url = {http://dx.doi.org/10.1016/j.fusengdes.2008.11.043},
volume = {84},
year = {2009}
}
