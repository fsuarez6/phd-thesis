pages = {298--303},
publisher = {IEEE},
shorttitle = {Intelligent Robots and Systems, 2001. Proceedings.},
title = {{Learning inverse kinematics}},
url = {http://ieeexplore.ieee.org/lpdocs/epic03/wrapper.htm?arnumber=973374},
